        android:layout_y="294dp"
        android:src="@drawable/input_off" />

    <ImageView
        android:id="@+id/imgInput1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="343dp"
        android:layout_y="243dp"
