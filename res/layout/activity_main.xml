        android:visibility="visible" />

    <Button
        android:id="@+id/btnStopRefresh"
        android:layout_width="82dp"
        android:layout_height="30dp"
        android:layout_marginLeft="58dp"
        android:layout_x="679dp"
        android:layout_y="186dp"
